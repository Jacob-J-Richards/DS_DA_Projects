RR[1,2] <- length(intersect(row.names(NO_RENT_INCREASE),row.names(NOT_RENEW)))
riskratio(RR[1,1], RR[1,2], nrow(RENT_INCREASE_LT_TEN), nrow(NO_RENT_INCREASE), conf.level=0.95, p.calc.by.independence=TRUE)
